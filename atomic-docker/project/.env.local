POSTGRES_USERNAME=postgres

# Supertokens
SUPERTOKENS_POSTGRESQL_CONNECTION_URI=postgresql://postgres:secretpgpassword@postgres:5432/postgres

# Hasura
HASURA_GRAPHQL_JWT_SECRET=some-long-secret-key-that-is-at-least-32-characters-long
HASURA_GRAPHQL_ADMIN_SECRET=myadminsecretkey

# Minio
STORAGE_ACCESS_KEY=myaccesskey
STORAGE_SECRET_KEY=mysecretkey
