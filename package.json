{
  "name": "atom-e2e-tests",
  "version": "1.0.0",
  "description": "E2E testing suite for Atom AI assistant personas",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:python": "cd atomic-docker/python-api && pytest",
    "dev": "cd atomic-docker/app_build_docker && npm run dev",
    "dev:atomic": "cd atomic-docker/app_build_docker && npm run dev",
    "build": "cd atomic-docker/app_build_docker && npm run build",
    "test:e2e": "./tests/run-e2e-tests.sh",
    "test:e2e:alex": "./tests/run-e2e-tests.sh --persona=alex",
    "test:e2e:maria": "./tests/run-e2e-tests.sh --persona=maria",
    "test:e2e:ben": "./tests/run-e2e-tests.sh --persona=ben",
    "test:e2e:all": "./tests/run-e2e-tests.sh --persona=all",
    "test:e2e:headless": "./tests/run-e2e-tests.sh --persona=all --headless",
    "test:setup": "npm install && npx playwright install",
    "test:e2e:playwright": "npx playwright test",
    "test:report": "npx allure serve tests/results",
    "test:integration": "node tests/integration-runner.js",
    "test:integration:web": "node tests/integration-runner.js --target=web",
    "test:integration:desktop": "node atomic-desktop/tauri/test-integration-complete.js --env=desktop",
    "test:comprehensive": "npm run test:integration && npm run test:e2e",
    "postinstall": "cd atomic-docker/app_build_docker && npm install",
    "orchestration": "node src/orchestration/index.js",
    "orchestration:demo": "node src/orchestration/FinancialOrchestrationDemo.js",
    "orchestration:demo:financial": "node src/orchestration/examples/FinancialPlanningExample.js",
    "orchestration:demo:business": "node src/orchestration/examples/BusinessAutomationExample.js",
    "orchestration:stats": "node scripts/orchestration-stats.js",
    "orchestration:optimize": "node scripts/orchestration-optimize.js",
    "orchestration:health": "node scripts/orchestration-health.js",
    "orchestration:reset": "node scripts/orchestration-reset.js",
    "orchestration:monitor": "node scripts/orchestration-monitor.js"
  },
  "dependencies": {
    "orchestration": "./src/orchestration",
    "@hubspot/api-client": "^13.0.0",
    "@microsoft/teams-ai": "^1.7.4",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "aws-cdk-lib": "^2.206.0",
    "axios": "^1.11.0",
    "botbuilder": "^4.17.0-blobs1",
    "openai": "^4.77.4",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "llamaindex": "^0.11.27",
    "node-cron": "^2.0.3",
    "form-data": "^4.0.4",
    "qrcode": "^1.5.3",
    "date-fns": "^3.6.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.54.2",
    "@types/async-retry": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "allure-commandline": "^2.24.1",
    "allure-jest": "^2.9.2",
    "allure-playwright": "^2.9.2",
    "babel-jest": "^30.0.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "@types/qrcode": "^1.5.5",
    "@types/node-cron": "^3.0.11",
    "@types/date-fns": "^2.6.0",
    "jest-allure": "^0.1.3",
    "mongoose": "^8.17.1",
    "playwright": "^1.40.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.2"
  },
  "allure": {
    "clean": true,
    "resultsDir": "tests/results/allure-results"
  }
}
