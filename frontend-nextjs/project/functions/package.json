{
  "name": "atom-functions",
  "version": "1.0.0",
  "description": "Atom Functions Service",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "build:functions": "npm run build",
    "start": "node dist/index.js",
    "start:functions": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "clean": "rm -rf dist",
    "test": "jest"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "http-proxy-middleware": "^2.0.6",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/node": "^18.15.11",
    "typescript": "^5.0.4",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.5.0",
    "@types/jest": "^29.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
